jira.SLA Time_to_resolution_DPJSM@ssu {
  name = "Time to resolution"
  config = {
    definition = {
      start = [
        {
          pluginKey = "com.atlassian.servicedesk"
          factoryKey = "issue-created-sla-condition-factory"
          conditionId = "issue-created-hit-condition"
          name = "Issue Created"
        },
        {
          pluginKey = "com.atlassian.servicedesk"
          factoryKey = "resolution-sla-condition-factory"
          conditionId = "resolution-cleared-hit-condition"
          name = "Resolution: Cleared"
        },
      ]
      stop = [
        {
          pluginKey = "com.atlassian.servicedesk"
          factoryKey = "resolution-sla-condition-factory"
          conditionId = "resolution-set-hit-condition"
          name = "Resolution: Set"
        },
      ]
    }
    goals = [
      {
        jqlQuery = "${ jira.Field.instance.Status__status } = \"${ jira.Status.instance.in_staging@s.name }\""
        duration = 216000000
        calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
        defaultGoal = false
        timeMetricId = 0
        subGoals = [
          {
            jqlQuery = "${ jira.Field.instance.Priority__priority } = ${ jira.Priority.instance.Highest.name }"
            duration = 43200000
            calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
            defaultGoal = false
          },
          {
            jqlQuery = "${ jira.Field.instance.Priority__priority } = ${ jira.Priority.instance.High.name }"
            duration = 86400000
            calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
            defaultGoal = false
          },
          {
            jqlQuery = "${ jira.Field.instance.Priority__priority } = ${ jira.Priority.instance.Medium.name }"
            duration = 129600000
            calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
            defaultGoal = false
          },
          {
            jqlQuery = "${ jira.Field.instance.Priority__priority } = ${ jira.Priority.instance.Low.name }"
            duration = 172800000
            calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
            defaultGoal = false
          },
        ]
      },
      {
        jqlQuery = ""
        duration = 288000000
        calendarId = jira.Calendar.instance.Sample_9_5_Calendar_DPJSM@sbsu
        defaultGoal = true
        timeMetricId = 0
      },
    ]
    slaDisplayFormat = "NEW_SLA_FORMAT"
  }
  _parent = [
    jira.Project.instance.DP_Service_Project@s,
  ]
  _alias = "DP Service Project:Time to resolution"
}
